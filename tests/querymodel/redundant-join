ddl
create table a(a, b); create unique index aa on a(a);
----

query apply=RedundantJoin
select * from a, a b where a.a = b.a;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a, a b where a.a = b.a;
 + RedundantJoin"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ DISTINCT TUPLES|0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q0.c0 AS A|3: Q0.c1 AS B| UNIQUE KEY [0]| UNIQUE KEY [2] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q0 [ label="(Q0.c0) = (Q0.c0)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:BaseTable A"
        boxhead1 [ shape = record, label="{ DISTINCT TUPLES|0: c0 AS A|1: c1 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q0 -> boxhead1 [ lhead = cluster1 ]
}

----
----

query apply=RedundantJoin
with a(a) as (select b from a group by b) select * from a, a b where a.a = b.a;
----
----
digraph G {
    compound = true
    labeljust = l
    label="with a(a) as (select b from a group by b) select * from a, a b where a.a = b.a;
 + RedundantJoin"
    node [ shape = box ]
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ DISTINCT TUPLES|0: Q4.c0 AS A|1: Q4.c0 AS A| UNIQUE KEY [0]| UNIQUE KEY [1] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q4 -> Q4 [ label="(Q4.c0) = (Q4.c0)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ DISTINCT TUPLES|0: Q3.c0 AS A| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ DISTINCT TUPLES|0: Q2.c0 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Grouping"
        boxhead2 [ shape = record, label="{ DISTINCT TUPLES|0: Q1.c1 AS B| GROUP Q1.c1 Ascending| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ DISTINCT TUPLES|0: Q0.c0 AS A|1: Q0.c1 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:BaseTable A"
        boxhead1 [ shape = record, label="{ DISTINCT TUPLES|0: c0 AS A|1: c1 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q4 -> boxhead4 [ lhead = cluster4 ]
    Q3 -> boxhead3 [ lhead = cluster3 ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
    Q1 -> boxhead0 [ lhead = cluster0 ]
    Q0 -> boxhead1 [ lhead = cluster1 ]
}

