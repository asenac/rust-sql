ddl
create table a(a, b, c);
----

query apply=ColumnRemoval
select a, c from (select * from a);
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a, c from (select * from a);
 + ColumnRemoval"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q1.c0 AS A|1: Q1.c1 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable A"
        boxhead4 [ shape = record, label="{ 0: c0 AS A|1: c2 AS C }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q1 -> boxhead3 [ lhead = cluster3 ]
    Q0 -> boxhead4 [ lhead = cluster4 ]
}

