ddl
create table a(a, b, c);
----

query apply=ColumnRemoval
select a, c from (select * from a);
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a, c from (select * from a);
 + ColumnRemoval"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q1.c0 AS A|1: Q1.c1 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A|1: c2 AS C }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q1 -> boxhead1 [ lhead = cluster1 ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

