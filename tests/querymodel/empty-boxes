query apply=EmptyBoxes
select * from a union all select * from a where false;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a union all select * from a where false;
 + EmptyBoxes"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q4.c0 AS A|1: Q4.c1 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ 0: Q2.c0 AS A|1: Q2.c1 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q4 -> boxhead5 [ lhead = cluster5 ]
    Q2 -> boxhead1 [ lhead = cluster1 ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

