ddl
create table a(a, b); create index aa on a(a);
----

query
select * from a;
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a;
"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

query
with b(b) as (select a from a) select * from b;
----
digraph G {
    compound = true
    labeljust = l
    label="with b(b) as (select a from a) select * from b;
"
    node [ shape = box ]
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ 0: Q2.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ 0: Q1.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q2 -> boxhead3 [ lhead = cluster3 ]
    Q1 -> boxhead1 [ lhead = cluster1 ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

