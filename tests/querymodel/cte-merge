    compound = true
    labeljust = l
    label="with b(b) as (select a from a), c(c) as (select b from b) select * from b;
"
    node [ shape = box ]
    subgraph cluster8 {
        label = "Box8:Select"
        boxhead8 [ shape = record, label="{ 0: Q4.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
        }
