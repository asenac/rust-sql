# @todo bug: USING clause projection
query apply=PushDownPredicates
select * from a left join b using(a) left join a c on b.a = c.a where b.b is null;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a left join b using(a) left join a c on b.a = c.a where b.b is null;
 + PushDownPredicates"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q4.c0 AS A|1: Q4.c1 AS B|2: Q4.c2 AS B|3: Q4.c3 AS A|4: Q4.c4 AS A|5: Q4.c5 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:OuterJoin"
        boxhead1 [ shape = record, label="{ 0: Q2.c0 AS A|1: Q2.c1 AS B|2: Q2.c2 AS B|3: Q2.c3 AS A|4: Q3.c0 AS A|5: Q3.c1 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(P)" ]
            Q3 [ label="Q3(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q2 -> Q3 [ label="(Q2.c3) = (Q3.c0)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:BaseTable A"
        boxhead5 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    subgraph cluster6 {
        label = "Box6:Select"
        boxhead6 [ shape = record, label="{ 0: Q5.c0 AS A|1: Q5.c1 AS B|2: Q5.c2 AS B|3: Q5.c3 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q5 [ label="Q5(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q5 -> Q5 [ label="(Q5.c2) IS NULL" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:OuterJoin"
        boxhead2 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q1.c1 AS B|3: Q1.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(P)" ]
            Q1 [ label="Q1(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q1 [ label="(Q0.c0) = (Q1.c0)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable B"
        boxhead4 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q4 -> boxhead1 [ lhead = cluster1 ]
    Q2 -> boxhead6 [ lhead = cluster6 ]
    Q3 -> boxhead5 [ lhead = cluster5 ]
    Q5 -> boxhead2 [ lhead = cluster2 ]
    Q0 -> boxhead3 [ lhead = cluster3 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
}

