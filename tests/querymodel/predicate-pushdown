query apply=PushDownPredicates
select a from (select a from a) group by a having a > 10
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a from (select a from a) group by a having a > 10
 + PushDownPredicates"
    node [ shape = box ]
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ DISTINCT TUPLES|0: Q3.c0 AS A| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Grouping"
        boxhead3 [ shape = record, label="{ DISTINCT TUPLES|0: Q2.c0 AS A| GROUP Q2.c0 Ascending| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q1.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q0 [ label="(Q0.c0) > (10)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q3 -> boxhead3 [ lhead = cluster3 ]
    Q2 -> boxhead0 [ lhead = cluster0 ]
    Q1 -> boxhead1 [ lhead = cluster1 ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

