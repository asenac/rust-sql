query apply=PushDownPredicates
select a.a from a left join b on true where a.a > 10
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a.a from a left join b on true where a.a > 10
 + PushDownPredicates"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q2.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:OuterJoin"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q1.c0 AS B| TRUE }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(P)" ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable B"
        boxhead3 [ shape = record, label="{ 0: c0 AS B }" ]
        {
            rank = same
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ 0: Q3.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q3 -> Q3 [ label="(Q3.c0) > (10)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q2 -> boxhead1 [ lhead = cluster1 ]
    Q0 -> boxhead4 [ lhead = cluster4 ]
    Q1 -> boxhead3 [ lhead = cluster3 ]
    Q3 -> boxhead2 [ lhead = cluster2 ]
}

