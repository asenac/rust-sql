query apply=PushDownPredicates
with b(b) as (select a from a) select * from (select * from (select * from b)) b, b c where b.b > 10;
----
----
digraph G {
    compound = true
    labeljust = l
    label="with b(b) as (select a from a) select * from (select * from (select * from b)) b, b c where b.b > 10;
 + PushDownPredicates"
    node [ shape = box ]
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ 0: Q4.c0 AS B|1: Q5.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
            Q5 [ label="Q5(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ 0: Q1.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q0.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:BaseTable A"
        boxhead1 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ 0: Q3.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ 0: Q2.c0 AS B }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q2 -> Q2 [ label="(Q2.c0) > (10)" ]
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q4 -> boxhead4 [ lhead = cluster4 ]
    Q5 -> boxhead2 [ lhead = cluster2 ]
    Q1 -> boxhead0 [ lhead = cluster0 ]
    Q0 -> boxhead1 [ lhead = cluster1 ]
    Q3 -> boxhead5 [ lhead = cluster5 ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
}

