query
select a from (select a from a union all select a from a) where a > 10;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a from (select a from a union all select a from a) where a > 10;
"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q5.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q5 [ label="Q5(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q5 -> Q5 [ label="(Q5.c0) > (10)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q4.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
        }
    }
    subgraph cluster6 {
        label = "Box6:Union"
        boxhead6 [ shape = record, label="{ 0: Q2.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
            Q3 [ label="Q3(F)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ 0: Q1.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:BaseTable A"
        boxhead5 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ 0: Q0.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q5 -> boxhead1 [ lhead = cluster1 ]
    Q4 -> boxhead6 [ lhead = cluster6 ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
    Q3 -> boxhead4 [ lhead = cluster4 ]
    Q1 -> boxhead5 [ lhead = cluster5 ]
    Q0 -> boxhead3 [ lhead = cluster3 ]
}

