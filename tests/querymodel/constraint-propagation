query apply=ConstraintPropagation
select * from a, b where a + b > 10 and a = c and b = d;
----
Err: expression not supported!

query apply=ConstraintPropagation
select * from a, b where a + b > 10 and a = c and b = a;
----
Err: expression not supported!

