query apply=(GroupByRemoval,Merge)
select a, b from a group by a;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a, b from a group by a;
 + GroupByRemoval + Merge"
    node [ shape = box ]
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ DISTINCT TUPLES|0: Q0.c0 AS A|1: Q0.c1 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster1 {
        label = "Box1:BaseTable A"
        boxhead1 [ shape = record, label="{ DISTINCT TUPLES|0: c0 AS A|1: c1 AS B| UNIQUE KEY [0] }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q0 -> boxhead1 [ lhead = cluster1 ]
}

