digraph G {
    compound = true
    labeljust = l
    label="select * from a union all select * from a union select a from a
"
    node [ shape = box ]
    subgraph cluster0 {
        label = "Box0:Select"
        boxhead0 [ shape = record, label="{ 0: Q6.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q6 [ label="Q6(F)" ]
        }
    }
    subgraph cluster7 {
        label = "Box7:Union"
        boxhead7 [ shape = record, label="{ DISTINCT TUPLES|0: Q3.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
            Q4 [ label="Q4(F)" ]
            Q5 [ label="Q5(F)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ 0: Q2.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster6 {
        label = "Box6:BaseTable A"
        boxhead6 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:Select"
        boxhead3 [ shape = record, label="{ 0: Q1.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable A"
        boxhead4 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q6 -> boxhead7 [ lhead = cluster7 ]
    Q3 -> boxhead1 [ lhead = cluster1 ]
    Q4 -> boxhead3 [ lhead = cluster3 ]
    Q5 -> boxhead5 [ lhead = cluster5 ]
    Q2 -> boxhead6 [ lhead = cluster6 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

