query
with b(b) as (select a from a) select * from b, b c, (with c(c) as (select b from b) select * from c, b where b.b = c.c) as d where b.b = c.b and b.b = d.c
----
----
digraph G {
    compound = true
    labeljust = l
    label="with b(b) as (select a from a) select * from b, b c, (with c(c) as (select b from b) select * from c, b where b.b = c.c) as d where b.b = c.b and b.b = d.c
"
    node [ shape = box ]
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ 0: Q2.c0 AS B|1: Q3.c0 AS B|2: Q8.c0 AS C|3: Q8.c1 AS B }" ]
