            edge [ arrowhead = none, style = solid ]
            Q2 -> Q2 [ label="(Q2.c3) IS NULL" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:OuterJoin"
        boxhead2 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q0.c2 AS C|3: Q1.c0 AS D|4: Q1.c1 AS E|5: Q1.c2 AS F }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(P)" ]
            Q1 [ label="Q1(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q0 [ label="(Q0.c2) > (10)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable B"
        boxhead4 [ shape = record, label="{ 0: c0 AS D|1: c1 AS E|2: c2 AS F }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B|2: c2 AS C }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
