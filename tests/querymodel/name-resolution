----
----

query
select a from a group by a asc
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a from a group by a asc
"
    node [ shape = box ]
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ DISTINCT TUPLES|0: Q2.c0 AS A| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Grouping"
        boxhead3 [ shape = record, label="{ DISTINCT TUPLES|0: Q1.c0 AS A| GROUP Q1.c0 Ascending| UNIQUE KEY [0] }" ]
        {
            rank = same
            node [ shape = circle ]
