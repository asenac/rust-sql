ddl
create table a(a, b, c); create table b(d, e, f);
----

query
select * from a
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a
"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q0.c2 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:BaseTable A"
        boxhead2 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B|2: c2 AS C }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q0 -> boxhead2 [ lhead = cluster2 ]
}

