# table A is hiden by the alias of the join
query
select a.a from (a left join b on c > 10) c;
----
Err: column A not found

# ... but the column is still visible through the alias or unqualified
query
select a from (a left join b on c > 10) c where c.d is null;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select a from (a left join b on c > 10) c where c.d is null;
"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q2.c0 AS A }" ]
        {
            rank = same
            node [ shape = circle ]
