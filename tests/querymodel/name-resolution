query
select b.z from (select a as z from a) b join a
----
----
digraph G {
    compound = true
    labeljust = l
    label="select b.z from (select a as z from a) b join a
"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q3.c0 AS Z }" ]
        {
            rank = same
            node [ shape = circle ]
            Q3 [ label="Q3(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ 0: Q1.c0 AS Z|1: Q2.c0 AS A|2: Q2.c1 AS B|3: Q2.c2 AS C }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster6 {
        label = "Box6:BaseTable A"
        boxhead6 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B|2: c2 AS C }" ]
        {
            rank = same
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ 0: Q0.c0 AS Z }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster5 {
        label = "Box5:BaseTable A"
        boxhead5 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B|2: c2 AS C }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q3 -> boxhead2 [ lhead = cluster2 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
    Q2 -> boxhead6 [ lhead = cluster6 ]
    Q0 -> boxhead5 [ lhead = cluster5 ]
}

