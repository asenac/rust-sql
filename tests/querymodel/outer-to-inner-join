query apply=(ConstraintPropagation,OuterToInnerJoin,Merge)
select * from a left join b on true where d = a
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a left join b on true where d = a
 + ConstraintPropagation + OuterToInnerJoin + Merge"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q1.c0 AS C|3: Q1.c1 AS D| TRUE }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
            Q1 [ label="Q1(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q1 [ label="(Q1.c1) = (Q0.c0)" ]
            Q1 -> Q1 [ label="NOT ((Q1.c1) IS NULL)" ]
            Q0 -> Q0 [ label="NOT ((Q0.c0) IS NULL)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable B"
        boxhead4 [ shape = record, label="{ 0: c0 AS C|1: c1 AS D }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q0 -> boxhead3 [ lhead = cluster3 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
}

