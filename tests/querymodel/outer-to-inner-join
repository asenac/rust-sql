ddl
create table a(a, b); create table b(c, d);
----

query apply=OuterToInnerJoin
select * from a left join b on a.a = b.c where b.d is not null;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a left join b on a.a = b.c where b.d is not null;
 + OuterToInnerJoin"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q2.c0 AS A|1: Q2.c1 AS B|2: Q2.c2 AS C|3: Q2.c3 AS D }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q2 -> Q2 [ label="NOT ((Q2.c3) IS NULL)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q1.c0 AS C|3: Q1.c1 AS D }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
            Q1 [ label="Q1(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q1 [ label="(Q0.c0) = (Q1.c0)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable B"
        boxhead4 [ shape = record, label="{ 0: c0 AS C|1: c1 AS D }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
    Q0 -> boxhead3 [ lhead = cluster3 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
}

----
----

query apply=OuterToInnerJoin-PushDownPredicates
select * from a left join b on a.a = b.c where b.d is not null;
----
----
digraph G {
    compound = true
    labeljust = l
    label="select * from a left join b on a.a = b.c where b.d is not null;
 + OuterToInnerJoin-PushDownPredicates"
    node [ shape = box ]
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ 0: Q2.c0 AS A|1: Q2.c1 AS B|2: Q2.c2 AS C|3: Q2.c3 AS D }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ 0: Q0.c0 AS A|1: Q0.c1 AS B|2: Q1.c0 AS C|3: Q1.c1 AS D }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
            Q1 [ label="Q1(F)" ]
            edge [ arrowhead = none, style = solid ]
            Q0 -> Q1 [ label="(Q0.c0) = (Q1.c0)" ]
            Q1 -> Q1 [ label="NOT ((Q1.c1) IS NULL)" ]
        }
    }
    subgraph cluster4 {
        label = "Box4:BaseTable B"
        boxhead4 [ shape = record, label="{ 0: c0 AS C|1: c1 AS D }" ]
        {
            rank = same
        }
    }
    subgraph cluster3 {
        label = "Box3:BaseTable A"
        boxhead3 [ shape = record, label="{ 0: c0 AS A|1: c1 AS B }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q2 -> boxhead2 [ lhead = cluster2 ]
    Q0 -> boxhead3 [ lhead = cluster3 ]
    Q1 -> boxhead4 [ lhead = cluster4 ]
}

